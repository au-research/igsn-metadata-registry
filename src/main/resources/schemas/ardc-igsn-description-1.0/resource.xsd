<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:ds="https://identifiers.ardc.edu.au/schemas/ardc-igsn-desc" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink/namespace" targetNamespace="https://identifiers.ardc.edu.au/schemas/ardc-igsn-desc" elementFormDefault="qualified" version="1.0">

    <xs:annotation>
        <xs:documentation> XML Schema v1.0 for registering resources with the ARDC IGSN service.</xs:documentation>
    </xs:annotation>
    <!-- xs:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="http://schemas.opengis.net/gml/3.2.1/basicTypes.xsd"/ -->
    <!-- xs:import namespace="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/XMLSchema-instance"/ -->
    <xs:element name="resources">
        <xs:annotation>
            <xs:documentation>A batch of resouces that will registered with IGSNs. </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="resource" minOccurs="1" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>A resource may refer to a physical sample (specimen), a collection, i.e., physical o logical grouping of physical samples, or a sampling feature (see OGC Observations and Measurements )</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:all>
                            <xs:element name="resourceIdentifier">
                                <xs:annotation>
                                    <xs:documentation>The IGSN code of a resource, e.g., CSRWASC111. The first two characters must be [A-Z] and specify the namespace of an allocating agent. The CS namespace has been assigned to CSIRO. This is followed by 3 characters [A-Z] representing the client, as designated by the allocating agent. The rest characters represent the local sample code specified by the client. This can be a combination of characters, numbers and dash (-) and dot (.). See the xsd pattern constraint.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="ds:igsnSyntax"/>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <!-- sample number, e.g., igsn -->
                            <xs:element name="landingPage">
                                <xs:annotation>
                                    <xs:documentation>The landing page of a resource. This page contains more detailed (domain-specific) information about the resource.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:anyURI">
                                        <xs:pattern value="https?://.+"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="isPublic" default="true">
                                <xs:annotation>
                                    <xs:documentation>The visibility of a resource's metadata. Use 'true' for public and 'false' for private resources.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:boolean">
                                            <xs:attribute name="embargoEnd" type="ds:W3CDTFdate">
                                                <xs:annotation>
                                                    <xs:documentation>A private resource will be not be publicly available until the embargo date.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="resourceTitle">
                                <xs:annotation>
                                    <xs:documentation>The name, descriptive phrase or title of the resource.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:token">
                                        <xs:minLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="alternateIdentifiers" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Alternate identifiers of the resource, including the local and other web-resolvable identifiers.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="alternateIdentifier" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:simpleContent>
                                                    <xs:extension base="ds:nonEmptyString">
                                                        <xs:attribute name="alternateIdentifierType" type="ds:identifierType" use="required"/>
                                                    </xs:extension>
                                                </xs:simpleContent>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="resourceTypes" minOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>The physical form of a resource, e.g. core, cuttings and grab. See the controlled list "include/resourceType.xsd".</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="resourceType" maxOccurs="unbounded">
                                            <xs:simpleType>
                                                <xs:union memberTypes="ds:resourceType ds:NilReasonType"/>
                                            </xs:simpleType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="materialTypes" minOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>The materials of which the resource is made, e.g., rock, sediment, and soil. See the controlled list "include/materialType.xsd"</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="materialType" maxOccurs="unbounded">
                                            <xs:simpleType>
                                                <xs:union memberTypes="ds:materialType ds:NilReasonType"/>
                                            </xs:simpleType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="classifications" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Domain-specific classification codes, i.e. other concepts describing the physical form, function or type of the resource. </xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="classification" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:simpleContent>
                                                    <xs:extension base="xs:token">
                                                        <xs:attribute name="classificationURI" type="xs:anyURI">
                                                            <xs:annotation>
                                                                <xs:documentation>You may use the attribute classifictionIdentifier to indicate the URI of the concept.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:attribute>
                                                    </xs:extension>
                                                </xs:simpleContent>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="purpose" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>The purpose for collecting/creating the resource.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:token">
                                        <xs:minLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="sampledFeatures" nillable="true" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>This element is mainly applicable to physical samples and sample collections. It represents a domain feature, e.g., a borehole core may sample multiple geologic units.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="sampledFeature" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:simpleContent>
                                                    <xs:extension base="xs:token">
                                                        <xs:attribute name="sampledFeatureURI" type="xs:anyURI">
                                                            <xs:annotation>
                                                                <xs:documentation>You may use this attribute to specify the link associated with a sampled feature.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:attribute>
                                                    </xs:extension>
                                                </xs:simpleContent>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="location" nillable="true" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>The location where the resource was found/collected (sampling feature,specimen) or created (synthetic materials). This may refer to a geo-location (in WKT) or a locality (aka. toponym). For a spatial location, the srid attribute may be used to indicate the spatial reference system (in EPSG). </xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:group ref="ds:locationGroup">
                                        <xs:annotation>
                                            <xs:documentation> </xs:documentation>
                                        </xs:annotation>
                                    </xs:group>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="date" nillable="true" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>The time period or date of the collection/creation of the resource.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:group ref="ds:timeGroup">
                                        <xs:annotation>
                                            <xs:documentation>Represents a time instant or a time period (start,end).</xs:documentation>
                                        </xs:annotation>
                                    </xs:group>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="method" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>The method/instrument by which the resource was collected/created, e.g., Dredging, Manual Observation. </xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:token">
                                            <xs:attribute name="methodURI" type="xs:anyURI"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="campaign" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>The field program/related projects, e.g., cruise or expedition; typically used for samples and sample collections.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:token">
                                        <xs:minLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="curationDetails" nillable="false">
                                <xs:annotation>
                                    <xs:documentation>Resource archival information, mainly applicable to physical samples and sample colletions. The curating instuition must be specified. This can be present or future curator.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="curation" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="curator" minOccurs="1">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="curatorName" minOccurs="1">
                                                                    <xs:simpleType>
                                                                        <xs:annotation>
                                                                            <xs:documentation>The full name of a resource curator.</xs:documentation>
                                                                        </xs:annotation>
                                                                        <xs:restriction base="xs:string">
                                                                            <xs:minLength value="1"/>
                                                                        </xs:restriction>
                                                                    </xs:simpleType>
                                                                </xs:element>
                                                                <xs:element name="curatorIdentifier" minOccurs="0">
                                                                    <xs:complexType>
                                                                        <xs:simpleContent>
                                                                            <xs:extension base="ds:nonEmptyString">
                                                                                <xs:attribute name="curatorIdentifierType" type="ds:identifierType" use="required"/>
                                                                            </xs:extension>
                                                                        </xs:simpleContent>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="curationDate" type="ds:W3CDTFdate" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>The date the resource was moved to its current location.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="curationLocation" type="xs:token" minOccurs="0">
                                                        <xs:annotation>
                                                            <xs:documentation>The location (address) where the resource is stored. This can be general or specific (room number, building name).</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="curatingInstitution">
                                                        <xs:complexType>
                                                            <xs:simpleContent>
                                                                <xs:extension base="xs:token">
                                                                    <xs:attribute name="institutionURI" type="xs:anyURI"/>
                                                                </xs:extension>
                                                            </xs:simpleContent>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="contributors" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Use this element to give recognition of those who contributed to the resource(s). Use contributor type 'originator' to indicate a sample collector/creator.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="contributor" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="contributorName">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:token">
                                                                <xs:minLength value="1"/>
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="contributorIdentifier" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:simpleContent>
                                                                <xs:extension base="ds:nonEmptyString">
                                                                    <xs:attribute name="contributorIdentifierType" type="ds:identifierType" use="required"/>
                                                                </xs:extension>
                                                            </xs:simpleContent>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                                <xs:attribute name="contributorType" type="ds:contributorType" use="required"/>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="relatedResources" minOccurs="0">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="relatedResource" maxOccurs="unbounded">
                                            <xs:annotation>
                                                <xs:documentation>Use this element to indicate related resources, documents, events, domain features, etc.</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="relatedResourceTitle" minOccurs="0">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:token"/>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="relatedResourceIdentifier">
                                                        <xs:complexType>
                                                            <xs:simpleContent>
                                                                <xs:extension base="ds:nonEmptyString">
                                                                    <xs:attribute name="relatedResourceIdentifierType" type="ds:identifierType" use="required"/>
                                                                </xs:extension>
                                                            </xs:simpleContent>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                                <xs:attribute name="relationType" type="ds:relationType" use="required">
                                                    <xs:annotation>
                                                        <xs:documentation>The relationship between the resource being registered and other entity (e.g., event, document, parent sample, etc.) </xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="comments" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Any free text describing the resource.This can also be used to include additional metadata which cannot be specified with the standard schema elements.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:token"/>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="logDate" nillable="false">
                                <xs:annotation>
                                    <xs:documentation>Use this element to indicate an event (the status of a resource or its registration) and its timestamp.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="ds:W3CDTFdate">
                                            <xs:attribute name="eventType" type="ds:eventType" use="required"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:all>
                        <xs:attribute name="registeredObjectType" type="ds:registrationType" use="required"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:group name="timeGroup">
        <xs:annotation>
            <xs:documentation>Represents a time instant or a time period (start,end).</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="timeInstant" type="ds:W3CDTFdate" minOccurs="0" maxOccurs="1"/>
            <xs:element name="timePeriod" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="start" type="ds:W3CDTFdate"/>
                        <xs:element name="end" type="ds:W3CDTFdate"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:choice>
    </xs:group>
    <xs:group name="locationGroup">
        <xs:annotation>
            <xs:documentation>This may apply to a named place (locality) or the geometry of a spatial location, e.g., WKT representation. The srid attribute indicates a Spatial Reference System Identifier (SRID). </xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="locality" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The named place/toponym or address. You may use the attribute localityIdentifier to denote the web location of a place.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:token">
                            <xs:attribute name="localityURI" type="xs:anyURI"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="geometry" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The Well Known Text (WKT) representation of a resource's location. For example: POINT(123.45 543.21). Use the following websites to determine the relevant EPSG codes of horizontal/vertical reference systems: (a) http://spatialreference.org (b) https://epsg.io/</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:token">
                            <xs:attribute name="srid" type="ds:spatialRefType" use="required">
                                <xs:annotation>
                                    <xs:documentation>This refers to the spatial referencing system identifier (SRID). Specify an EPSG code, e.g., "4326" is the EPSG code for the WGS84 referenced coordinate system.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="verticalDatum" type="ds:spatialRefType" use="optional">
                                <xs:annotation>
                                    <xs:documentation>A vertical datums specify the origin for elevations assocated with a resource. Specify the EPSG vertical coordinate reference system code. For example, 5711 refers is the epsg code of the Australian Height Datum (AHD). </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="geometryURI" type="xs:anyURI"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:all>
    </xs:group>
    <xs:simpleType name="igsnSyntax">
        <xs:annotation>
            <xs:documentation>IGSN syntax data type</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:pattern value="([0-9\.]+\/[A-Z]{2}[A-Za-z0-9\-.]+)"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="nonEmptyString">
        <xs:restriction base="xs:token">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="contributorType" id="contributorType">
        <xs:annotation>
            <xs:documentation>The type of contributor of the resource (adapted from datacite-contributorType-v4.0.xsd'). A full list of definitions available at http://pid.geoscience.gov.au/def/voc/ga/igsncode/contributorType</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:anyURI">
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/pointOfContact"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/custodian"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/funder"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/processor"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/principalInvestigator"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/contributor"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/collaborator"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/rightsHolder"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/owner"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/sponsor"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/stakeholder"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/user"/>
            <xs:enumeration value="http://registry.it.csiro.au/def/isotc211/CI_RoleCode/originator"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:annotation>
        <xs:documentation>Temporal information (instant and inetrval) (modified from igsn-dateType-v1.1.xsd)</xs:documentation>
    </xs:annotation>
    <xs:simpleType name="dateTimeWithoutMinutes">
        <xs:annotation>
            <xs:documentation>2011-10-05T08:00</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{4}-\d\d-\d\dT\d\d:\d\d"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="dateTimeWithoutMinutesPlusTimeZone">
        <xs:annotation>
            <xs:documentation>2011-10-05T08:00+00:00</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{4}-\d\d-\d\dT\d\d:\d\d\+\d\d:\d\d"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W3CDTFdate">
        <xs:annotation>
            <xs:documentation>Definition of W3CDTF compliant date. see: http://www.w3.org/TR/NOTE-datetime</xs:documentation>
        </xs:annotation>
        <xs:union memberTypes="xs:gYear xs:gYearMonth xs:date xs:dateTime ds:dateTimeWithoutMinutes ds:dateTimeWithoutMinutesPlusTimeZone"/>
    </xs:simpleType>

    <xs:simpleType name="eventType">
        <xs:annotation>
            <xs:documentation>The status type of a resource and its registration. </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="registered"/>
            <xs:enumeration value="updated"/>
            <xs:enumeration value="deprecated"/>
            <xs:enumeration value="destroyed"/>
            <!--  registered - The date of registration.  -->
            <!--  updated - Date of the last metadata update.  -->
            <!--  deprecated - The resource registration is deprecated. The resource is no longer relevant, e.g. due to duplicate registration.  -->
            <!--  destroyed - The resource is destroyed.  -->
            <!-- xs:enumeration value="registered"/ -->
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="geometryType">
        <xs:restriction base="xs:anyURI">
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/Point"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/Linestring"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/MultiPoint"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/Polygon"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/Geometry"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/GeometryCollection"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/Curve"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/Line"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/LinearRing"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/MultiCurve"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/MultiLineString"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/Surface"/>
            <xs:enumeration value="http://pid.geoscience.gov.au/def/voc/ga/igsncode/MultiSurface"/>
